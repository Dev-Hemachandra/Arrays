#include<stdio.h>
int main(){
    int arr[]={1,3,5,6,7};
     int n=5,search,found=0;
     printf("Enter the value what you want to search:");
     scanf("%d",&search);
     for(int i=0;i<n;i++){
         if(arr[i]==search){
         printf("Element is found at:%d",i);
         found=1;
         break;}}
         if(found==0){
         printf("Element is not found");}
     return 0;
}
