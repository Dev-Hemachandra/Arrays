#include <stdio.h>
int main()
{
 int arr[10] = { 12, 16, 20, 40, 50, 70 };
 int capacity = sizeof(arr) / sizeof(arr[0]);
 int n = 6;
 int i, key = 26;
 printf("\n Before Insertion: ");
 for (i = 0; i < n; i++)
 printf("%d ", arr[i]);

if (n >= capacity)
 {printf("\nElement cannot be inserted");}
 else{
 arr[n] = key;
 n = n+1;}
 printf("\n After Insertion: ");
 for (i = 0; i < n; i++)
 printf("%d ", arr[i]);
 return 0;
}
