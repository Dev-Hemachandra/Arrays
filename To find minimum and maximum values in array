#include<stdio.h>
int main(){
    int n;
    printf("Enter the number of elements in an array:");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        printf("student marks %d:",i+1);
        scanf("%d",&arr[i]);
    }
    int min=arr[0];
    int max=arr[0];
    for(int i=0;i<n;i++){
        if(min>arr[i]){
            min=arr[i];
        }
        if(max<arr[i]){
            max=arr[i];
        }
    }
    printf("Minimun value is:%d\n",min);
    printf("Maximum value is:%d",max);
    return 0;
}
